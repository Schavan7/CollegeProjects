ody">                            <div ng-bind-html="library.description|convertEntities">                            </div>                            <!--                            <div>                                {{library.created | date:\'fullDate\'}} {{library.created | date:\'shortTime\'}}                            </div>                                -->                        </div>                        <div class="library-card-modified">                            <span class="library-card-modified-desc">                                Modified on:                            </span>                            <span title="{{library.modified| date:\'medium\'}}" class="library-card-modified-date">                                {{library.modified | date:\'mediumDate\'}}                            </span>                        </div>                    </div>                    <div ng-if="$ctrl.libraries.length == 0 && !$ctrl.isOwner" role="row">                        This user has no libraries.                    </div>                    <div ng-if="$ctrl.libraries.length == 0 && $ctrl.isOwner" role="row" class="library-card">                        <div class="profile-no-libraries">                            <a href="" ng-click="$ctrl.add()">                                <div class="glyphicon glyphicon-plus-sign profile-no-library-plus"></div>                                <div>                                    You have no libraries.                                    Click here to create a library!                                </div>                            </a>                        </div>                    </div>                    <div class="pull-right">                        <a href="/{{$ctrl.user.id}}/libraries">View all</a>                    </div>                </div>            </div>        </div>    </div></maybe-not-found>');
